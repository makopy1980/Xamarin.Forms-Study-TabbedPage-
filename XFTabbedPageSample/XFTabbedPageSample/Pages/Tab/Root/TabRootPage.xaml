<?xml version="1.0" encoding="UTF-8"?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms" 
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
	xmlns:tabpages="clr-namespace:XFTabbedPageSample.Pages.Tab"
	x:Class="XFTabbedPageSample.Pages.Tab.Root.TabRootPage">
	<TabbedPage.Children>
		<tabpages:TabFirstPage Icon="TabIcon1.png" />
		<tabpages:TabSecondPage Icon="TabIcon2.png" />
		<NavigationPage Title="Third" Icon="TabIcon3.png">
			<x:Arguments>
				<tabpages:TabThirdPage />
			</x:Arguments>
		</NavigationPage>
	</TabbedPage.Children>
</TabbedPage>